#include <string>
#include <iostream>
using namespace std;

class tissuebox
{
public:
    int numTissues(); const{return numTissues;}
    int currentTissues(); const{return currentTissues;}
    int maxTissues(); const{return maxTissues;}
    
    getDimensions(){
        cout << "What is the length of the tissuebox?: ";
        cin >> length;
        cout << "What is the width of the tissuebox?: ";
        cin >> width;
        return length, width;
    }
    
    
    int useTissue()
    {
        currentTissues = currentTissues - 1;
        return currentTissues;
    }
    
private:
    int numTissues, currentTissues, maxTissues;

};
